{
    "apiVersion": "1.0",
    "version": "2.0.0",
    "name": "Well Log v2.0 - Unpivot",
    "id": "spotfire-well-log-mod-unpivot",
    "icon": "icon.svg",
    "externalResources": ["https://fonts.gstatic.com/s/materialicons/v50/flUhRq6tzZclQEJ-Vdg-IuiaDsNc.woff2"],    
    "properties": [
        {
            "name": "verticalZoomHeightMultiplier",
            "type": "double",
            "defaultValue": 2.0
        },
        {
            "name": "template1",
            "type": "string",
            "defaultValue": "empty"
        },
        {
            "name": "template2",
            "type": "string",
            "defaultValue": "empty"
        },
        {
            "name": "template3",
            "type": "string",
            "defaultValue": "empty"
        },
        {
            "name": "template4",
            "type": "string",
            "defaultValue": "empty"
        },
        {
            "name": "template5",
            "type": "string",
            "defaultValue": "empty"
        },
        {
            "name": "template6",
            "type": "string",
            "defaultValue": "empty"
        },
        {
            "name": "template7",
            "type": "string",
            "defaultValue": "empty"
        }
    ],
    "dataViewDefinition": {
        "axes": [
            {
                "name": "DEPTH",
                "mode": "continuous",
                "allowMultipleMeasures": false,
                "placement": "left",
                "propertyControl": {
                    "title": "Depth Axis"
                },
                "allowNonAggregatingMeasures": true,
                "legendItem": {
                    "title": "Depth Axis",
                    "defaultVisibility": "visible"
                }
            },
            {
                "name": "Category",
                "mode": "categorical",
                "allowMultipleMeasures": false,
                "placement": "right",
                "propertyControl": {
                    "title": "Category"
                },
                "allowNonAggregatingMeasures": true,
                "legendItem": {
                    "title": "Category",
                    "defaultVisibility": "visible"
                }
            },
            {
                "name": "Value",
                "mode": "categorical",
                "allowMultipleMeasures": false,
                "placement": "bottom",
                "propertyControl": {
                    "title": "Value"
                },
                "allowNonAggregatingMeasures": true,
                "legendItem": {
                    "title": "Value",
                    "defaultVisibility": "visible"
                }
            },
            {
                "name": "WELL",
                "mode": "categorical",
                "allowMultipleMeasures": false,
                "placement": "top",
                "propertyControl": {
                    "title": "WELL"
                },
                "allowNonAggregatingMeasures": true,
                "legendItem": {
                    "title": "WELL",
                    "defaultVisibility": "visible"
                }
            }
        ]
    },
    "files": [
        "index.html",
        "main.css",
        "jquery-ui.css",
        "bundle.js"
    ]
}